        <item>@array/movie_list</item>
    </array>


    <array name="movie_list">
        <item>@array/youTube</item>
        <item>@array/twitch</item>
        <item>@array/dailymotion</item>
        <item>@array/megogo</item>
        <item>@array/vidio</item>
        <item>@array/muz1</item>
        <item>@array/m24</item>
        <item>@array/kinogo</item>
