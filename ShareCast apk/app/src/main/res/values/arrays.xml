        <item>@array/ted</item>
        <item>@array/liveleak</item>
        <item>@array/kinokrad</item>
    </array>



    <string-array name="youTube">
        <item name="imgPath">@drawable/youtube</item>
        <item name="url">https://www.youtube.com</item>
        <item name="match">videoplayback|m3u8</item>
    </string-array>

